{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "quiet": true
      }
    },
    "typeCheck": false,
    "assets": [
      {
        "include": "package.json",
        "exclude": "node_modules/**/*",
        "outDir": "dist"
      }
    ]
  }
}